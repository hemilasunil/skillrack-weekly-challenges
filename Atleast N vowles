#include<stdio.h>
#include<string.h>

int main()
{
 char a[1000];
 int n,i,c=0,l;
 scanf("%s",a);
 l=strlen(a);
 scanf("%d",&n);
 for(i=0;i<l;i++)
 {
     if(a[i]=='a'||a[i]=='A'||a[i]=='e'||a[i]=='E'||a[i]=='i'||a[i]=='I'||a[i]=='o'||a[i]=='O'||a[i]=='u'||a[i]=='U')
     {
         c++;
     }
 }
 if(c>=n)
 {
     printf("Yes");
 }
 else
 {
     printf("No");
 }
 

}
